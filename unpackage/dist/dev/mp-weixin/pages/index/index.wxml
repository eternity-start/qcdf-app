<view class="home"><swiper circular="{{true}}" autoplay="{{true}}" indicator-dots="{{true}}" indicator-color="#ffffff" indicator-active-color="#003952"><block wx:for="{{swiper}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><swiper-item><image src="{{item.img}}" mode></image></swiper-item></block></swiper><view class="nav"><block wx:for="{{navs}}" wx:for-item="items" wx:for-index="index" wx:key="index"><view class="item"><view class="{{[items.icons]}}"></view><text>{{items.title}}</text></view></block></view><view class="recommend"><view class="title"><text>推荐商品</text></view><view class="goods-list"><block wx:for="{{goods}}" wx:for-item="items" wx:for-index="__i1__" wx:key="id"><view class="goods-item"><image src="{{items.img_url}}" mode></image><view class="price"><text>{{items.sell_price}}</text><text>{{items.market_price}}</text></view><view class="name">{{''+items.title+''}}</view></view></block></view></view></view>