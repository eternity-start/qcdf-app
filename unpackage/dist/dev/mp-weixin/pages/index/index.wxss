.home .swiper1 {
  height: 408rpx;
}
.home .swiper1 image {
  width: 100%;
  height: 100%;
}
.home .nav {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.home .nav .item {
  margin-top: 10rpx;
  width: 25%;
  text-align: center;
}
.home .nav .item .iconfont {
  font-size: 28px;
  color: #ffffff;
  width: 120rpx;
  height: 120rpx;
  background-color: #e11e0c;
  line-height: 120rpx;
  border-radius: 100rpx;
  margin: 0 auto;
}
.home .recommend .title {
  border-top: 8px solid #eee;
  border-bottom: 8px solid #eee;
  text-align: center;
  line-height: 80rpx;
  letter-spacing: 20px;
  color: #e11e0c;
  font-size: 38rpx;
}

